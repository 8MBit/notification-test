{% extends 'base.html.twig' %}

{% block body %}
    <div class="wrapper">
        <div class="header">
            <h1>Notification List</h1>
            <a href="/" class="button button-orange">VIEW ACTIVE NOTIFICATIONS</a>
        </div>
        <div class="container">
        </div>
    </div>
    <script>
        window.notifications = JSON.parse('{{ notifications|raw|escape('js') }}');
    </script>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="/js/list.js"></script>
{% endblock %}
